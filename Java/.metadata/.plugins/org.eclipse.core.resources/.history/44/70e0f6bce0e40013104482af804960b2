import java.util.Scanner;


public class Task01 {

	public static void main(String[] args) {
		Scanner input = new Scanner(System.in);
		int hours = 0;
		int minutes = 0;
		while (input.nextLine() != "End") {
			hours += input.nextInt();
			minutes += input.nextInt();
		}
		
		System.out.println(hours + ":" + minutes);
	}

}
